<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <sys:Double x:Key="H1FontSize">56</sys:Double>
    <sys:Double x:Key="H2FontSize">32</sys:Double>
    <sys:Double x:Key="H3FontSize">20</sys:Double>
    <sys:Double x:Key="H4FontSize">18</sys:Double>
    <sys:Double x:Key="H5FontSize">16</sys:Double>
    <sys:Double x:Key="H6FontSize">14</sys:Double>
    <sys:Double x:Key="H7FontSize">12</sys:Double>
    <FontFamily x:Key="Password">password.ttf#Password</FontFamily>
    <FontFamily x:Key="RobotoRegular">Roboto-Regular.ttf#Roboto</FontFamily>
    <FontFamily x:Key="RobotoItalic">Roboto-Italic.ttf#Roboto Italic</FontFamily>
    <FontFamily x:Key="RobotoMedium">Roboto-Medium.ttf#Roboto Medium</FontFamily>
    <FontFamily x:Key="RobotoMediumItalic">Roboto-MediumItalic.ttf#Roboto Medium Italic</FontFamily>
    <FontFamily x:Key="RobotoBold">RobotoBold.ttf#Roboto Bold</FontFamily>
    <FontFamily x:Key="RobotoBoldItalic">Roboto-BoldItalic.ttf#Roboto Bold Italic</FontFamily>
</ResourceDictionary>