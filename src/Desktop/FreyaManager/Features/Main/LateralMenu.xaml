    </UserControl.Resources>
    <Grid>
        <d:Grid.DataContext>
            <local:LateralMenuViewModelForDesing IsMenuVisible="True" />
        </d:Grid.DataContext>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid Visibility="{Binding IsMenuVisible, Converter={StaticResource BooleanToVisibilityConverter}}"
              Background="White">
            <StackPanel MinWidth="250" Background="{StaticResource primary1ColorBrush}">
                <Grid 
                      Height="60"
                      UIElement.TouchUp="Grid_TouchUp"
                      UIElement.MouseUp="Grid_MouseUp">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <!--<Image Source="{StaticResource WorkforceDrawingImage}"
                       Margin="16,20" />-->
                    <StackPanel Orientation="Vertical"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Margin="16,0,0,0">
                        <TextBlock Text="Gestor interno"
                                   d:Text="Gestor interno"
                                   FontSize="{StaticResource H3FontSize}"
                                   FontFamily="{StaticResource RobotoBold}"
                                   Foreground="White"
                                   Margin="0,0,0,0" />
                    </StackPanel>
                    <Image Source="{StaticResource MenuOpenIcon}"
                           Grid.Column="1"
                           Width="24"
                           Margin="16,0" />
                </Grid>
                <controls:TextImageButton Text="Compañias"
                                          HorizontalAlignment="Left"
                                          Margin="16,24,0,0"
                                          ImageWidth="32"
                                          FontSize="20"
                                          HorizontalContentAlignment="Left"
                                          Width="220"
                                          Style="{StaticResource MediumPrimaryButton}"
                                          StartImageSource="{StaticResource HomeWhite}"
                                          Command="{Binding GoToCompaniesCommand }" />
                <controls:TextImageButton Text="Subscripciones"
                                          HorizontalAlignment="Left"
                                          Margin="16,24,0,0"
                                          ImageWidth="32"
                                          Width="220"
                                          HorizontalContentAlignment="Left"
                                          Style="{StaticResource MediumPrimaryButton}"
                                          FontSize="20"
                                          StartImageSource="{StaticResource HomeWhite}"
                                          Command="{Binding GoToSubscriptionsCommand }" />
                <controls:TextImageButton Text="FAQ"
                                          HorizontalAlignment="Left"
                                          HorizontalContentAlignment="Left"
                                          Margin="16,24,0,0"
                                          ImageWidth="32"
                                          Width="220"
                                          Style="{StaticResource MediumPrimaryButton}"
                                          FontSize="20"
                                          StartImageSource="{StaticResource HomeWhite}"
                                          Command="{Binding GoToFaqCommand }" />
                <controls:TextImageButton Text="Ajustes"
                                          HorizontalAlignment="Left"
                                          HorizontalContentAlignment="Left"
                                          Margin="16,24,0,0"
                                          ImageWidth="32"
                                          Width="220"
                                          Style="{StaticResource MediumPrimaryButton}"
                                          FontSize="20"
                                          StartImageSource="{StaticResource HomeWhite}"
                                          Command="{Binding GoToSettingsCommand }" />
                
            </StackPanel>
            <Grid VerticalAlignment="Bottom"
                  Margin="16,24">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <Grid Margin="8,0"
                      Height="50">
                    <StackPanel Orientation="Vertical"
                                HorizontalAlignment="Left">
                        <TextBlock Text="{Binding Employee.Email}"
                                   d:Text="mbmdevelop@gmail.com"
                                   FontSize="{StaticResource H6FontSize}"
                                   Margin="0,0,0,4"
                                   Foreground="White" />
                        <TextBlock Text="Gestor del sistema"
                                   FontSize="{StaticResource H7FontSize}"
                                   Foreground="White" />
                    </StackPanel>
                </Grid>
                <controls:IconButtonControl ImageSource="{StaticResource LogoutWhite}"
                                            VerticalAlignment="Center"
                                            Command="{Binding CloseSessionCommand}"
                                            Style="{StaticResource SmallPrimaryStyleIconButton}"
                                            Grid.Column="1" />
            </Grid>

        </Grid>
        <Grid Height="60"
              VerticalAlignment="Top"
              Visibility="{Binding IsMenuVisible,Converter={StaticResource BooleanToVisibilityConverter}, ConverterParameter=inverse}">
            <Image Source="{StaticResource MenuIcon}"
                   Visibility="{Binding ShowBackButton,ElementName=self,Converter={StaticResource BooleanToVisibilityConverter}, ConverterParameter=inverse}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Center"
                   UIElement.TouchUp="Grid_TouchUp"
                   Margin="0,5,0,0"
                   UIElement.MouseUp="Grid_MouseUp"
                   Height="55" />
            <Grid Height="60"
                  Width="60"
                  Visibility="{Binding ShowBackButton,ElementName=self,Converter={StaticResource BooleanToVisibilityConverter}}"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  UIElement.TouchUp="Grid_back_TouchUp"
                  UIElement.MouseUp="Grid_back_MouseUp">
                <Image Source="{StaticResource BackIcon}"
                       Width="45"
                       Height="45" />
            </Grid>
        </Grid>
        <Grid Background="{StaticResource disabledColorBrush}"
              Grid.Column="1"
              Visibility="{Binding IsMenuVisible,Converter={StaticResource BooleanToVisibilityConverter}}"
              VerticalAlignment="Stretch"
              HorizontalAlignment="Stretch"
              UIElement.TouchUp="Grid_TouchUp"
              UIElement.MouseUp="Grid_MouseUp" />
        
        
    </Grid>
</UserControl>
